  "description": "TodoMVC of React by thsi",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "pretest": "npm run clean && cross-env NODE_ENV=pretest npm run compile",
    "predevtest": "npm run clean && cross-env NODE_ENV=predevtest npm run compile",
    "start": "cross-env node build/scripts/start",
    "test": "cross-env NODE_ENV=test karma start build/karma.config",
    "test:watch": "npm test -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "mocks": "json-server --watch ./mocks/db.js --config ./mocks/json-server-config.json"
  },
